<section class="wrapper">
  <div class="row">
    <div class="col-lg-12">
      <h3 class="page-header">
      </h3>
      <ol class="breadcrumb">
        <li><i class="fa fa-home mx-2"></i>Home</li>
        <li><i class="fa fa-laptop me-2"></i>Dashboard</li>
      </ol>
    </div>
  </div>
  <p id="notice"><%= notice %></p>
  <p>
    <strong>Id project:</strong>
    <%= @activity.project_id %>
  </p>
  <p>
    <strong>Name activity:</strong>
    <%= @activity.name_activity %>
  </p>
  <p>
    <strong>Start date:</strong>
    <%= @activity.start_date.strftime("%d-%m-%Y") %>
  </p>
  <p>
    <strong>End date:</strong>
    <%= @activity.end_date.strftime("%d-%m-%Y") %>
  </p>
  <p>
    <strong>Finished:</strong>
    <%= @activity.finished %>
  </p>
  <%= link_to 'Edit', edit_activity_path(@activity) %> |
  <%= link_to 'Back', activities_path %>
